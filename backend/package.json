{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "PORT=3001 nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "PORT=3001 node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.ts",
    "test:integration": "jest --config ./test/jest-integration.config.ts",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts",
    "db:push": "prisma db push",
    "db:reset": "prisma migrate reset",
    "db:migrate:test": "DATABASE_URL=$TEST_DATABASE_URL prisma migrate deploy",
    "db:migrate:e2e": "DATABASE_URL=$E2E_DATABASE_URL prisma migrate deploy",
    "db:seed:e2e": "DATABASE_URL=$E2E_DATABASE_URL prisma db seed",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "setup": "pnpm install && pnpm docker:up && sleep 5 && pnpm prisma:generate && pnpm prisma:migrate",
    "clean": "rm -rf node_modules dist"
  },
